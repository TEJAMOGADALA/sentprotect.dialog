<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
  <Id>bfd7f930-2085-4ee0-8681-ca2042a1991f</Id>
  <Version>0.0.0.2</Version>
  <ProviderName>EY</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="EY SendProtect"/>
  <Description DefaultValue="A template to get started with EY SendProtect."/>
  <IconUrl DefaultValue="https://gray-moss-0578a810f.6.azurestaticapps.net/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://gray-moss-0578a810f.6.azurestaticapps.net/assets/icon-128.png"/>
  <SupportUrl DefaultValue="https://eyt.service-now.com/ey"/>
  <AppDomains>
    <AppDomain>https://*.ey.com</AppDomain>
    <AppDomain>https://gray-moss-0578a810f.6.azurestaticapps.net</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>

  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <Permissions>ReadWriteMailbox</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit" />
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Edit" />
  </Rule>

  <VersionOverrides>
    <VersionOverride xsi:type="VersionOverridesV1_0">
      <Hosts>
        <Host xsi:type="MailHost">
          <DesktopFormFactor>
            <ExtensionPoint xsi:type="MessageRead">
              <SourceLocation DefaultValue="https://gray-moss-0578a810f.6.azurestaticapps.net/"/>
              <RequestedHeight>250</RequestedHeight>
            </ExtensionPoint>

            <ExtensionPoint xsi:type="Events">
              <Event Type="ItemSend" FunctionName="OnMessageSend" />
              <Event Type="AppointmentSend" FunctionName="OnAppointmentSend" />
            </ExtensionPoint>
          </DesktopFormFactor>
        </Host>
      </Hosts>
    </VersionOverride>
  </VersionOverrides>
</OfficeApp>